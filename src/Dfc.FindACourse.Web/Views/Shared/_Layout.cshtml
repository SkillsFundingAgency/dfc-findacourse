    <link href="/public/stylesheets/application.css" media="screen" rel="stylesheet" type="text/css" /><!--<![endif]-->
    <link href="~/css/application.css" media="screen" rel="stylesheet" type="text/css" />
    <link href="~/css/icons.css" media="screen" rel="stylesheet" type="text/css" />

    <link rel="stylesheet" href="https://code.jquery.com/ui/1.12.0/themes/smoothness/jquery-ui.css" />

    <meta property="og:image" content="~/images/govuk-opengraph-image.png">
</head>
<body class="govuk-template__body">
    <script>document.body.className = ((document.body.className) ? document.body.className + ' js-enabled' : 'js-enabled');</script>

    <a href="#main-content" class="govuk-skip-link">Skip to main content</a>

    <partial name="_HeaderPartial" />

    <div class="govuk-width-container">
        <main class="govuk-main-wrapper " id="main-content" role="main">
            @RenderBody()
        </main>
    </div>

    <partial name="_FooterPartial" />
    <partial name="_ScriptsPartial" />

